{
  "all": true,
  "require": [
    "babel-register"
  ],
  "sourceMap": false,
  "instrument": false,
  "reporter": [
    "lcov",
    "text-summary",
    "text"
  ],
  "extension": [
      ".js"
  ],
  "include": [
      "src/**/*.js"
  ],
  "exclude": [
    "test/**/*"
  ],
  "watermarks": {
    "lines": [99, 100],
    "branches": [99, 100],
    "functions": [99, 100],
    "statements": [99, 100]
  },
  "report-dir": "./build/reports/coverage",
  "temp-directory": "./build/tmp/.nyc_output"
}